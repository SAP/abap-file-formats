# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# ensure the LF line endings
[*.{abap,md,properties,acds,json,mjs,gitignore,yml}]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true


[*.json]
indent_style = space
indent_size = 2
