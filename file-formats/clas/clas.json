{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/SAP/abap-file-formats/blob/main/file-formats/clas/clas.json",
  "title": "Interface Properties",
  "description": "Interface properties",
  "type": "object",
  "properties": {
    "$schema": {
      "title": "Schema",
      "description": "Format version",
      "type": "string"
    },
    "header": {
      "title": "Header",
      "description": "header",
      "type": "object",
      "properties": {
        "description": {
          "title": "Description",
          "description": "Description of the ABAP object",
          "type": "string",
          "maxLength": 60
        },
        "originalLanguage": {
          "title": "Original Language",
          "description": "Original language of the ABAP object",
          "type": "string",
          "maxLength": 2
        },
        "abapLanguageVersion": {
          "title": "ABAP Language Version (source code object)",
          "description": "ABAP language version for source code objects",
          "type": "string",
          "enum": [
            "standard",
            "keyUser",
            "cloudDevelopment"
          ],
          "enumDescriptions": [
            "Standard",
            "ABAP for key user extensibility",
            "ABAP cloud development"
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "description",
        "originalLanguage",
        "abapLanguageVersion"
      ]
    },
    "category": {
      "title": "Class Category",
      "description": "Class category",
      "type": "string",
      "enum": [
        "generalObjectType",
        "exitClass",
        "persistentClass",
        "factoryForPersistentClass",
        "statusClassForPersistClass",
        "viewClass",
        "proxyClassForRemoteIntf",
        "rfcProxyClass",
        "exceptionClass",
        "businessClass",
        "businessClassIntfForStatic",
        "businessClassIntfForInstan",
        "bspApplicationClass",
        "basisClassBspElementHdlr",
        "webDynproRuntimeObject",
        "esiProviderIntfGenerated",
        "areaClassSharedObjects",
        "testclassAbapUnit",
        "intfForParameterTypes",
        "behaviorClass"
      ],
      "enumDescriptions": [
        "General object type",
        "Exit class",
        "Persistent class",
        "Factory for persistent class",
        "Status class for persistent class",
        "View class",
        "Proxy class for remote interface",
        "RFC proxy class",
        "Exception class",
        "Business class",
        "Business class interface for static components",
        "Business class interface for instance-dependent components",
        "BSP application class",
        "Basis class for BSP element handlers",
        "Web Dynpro runtime object",
        "ESI: Provider interface (generated)",
        "Area class (shared objects)",
        "Test class (ABAP Unit)",
        "Interface for parameter types for database procedure proxies",
        "Behavior class (behavior implementations)"
      ]
    },
    "fixPointArithmetic": {
      "title": "Fix Point Arithmetic",
      "description": "Fix point arithmetic",
      "type": "boolean"
    },
    "messageClass": {
      "title": "Message Class",
      "description": "The message class of the class",
      "type": "string",
      "maxLength": 20
    },
    "types": {
      "title": "Type Descriptions",
      "description": "Type descriptions",
      "type": "array",
      "items": {
        "title": "Name and Description",
        "description": "Name and description",
        "type": "object",
        "properties": {
          "name": {
            "title": "Name",
            "description": "Name",
            "type": "string",
            "maxLength": 30
          },
          "description": {
            "title": "Description",
            "description": "Description",
            "type": "string",
            "maxLength": 60
          }
        },
        "additionalProperties": false,
        "required": [
          "name",
          "description"
        ]
      }
    },
    "attributes": {
      "title": "Attribute Descriptions",
