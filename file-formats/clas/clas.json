            "maxLength": 60
          },
          "parameters": {
            "title": "Parameter Descriptions",
            "description": "Parameter descriptions",
            "type": "array",
            "items": {
              "title": "Name and Description",
              "description": "Name and description",
              "type": "object",
              "properties": {
                "name": {
                  "title": "Name",
