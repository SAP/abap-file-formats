{
  "input_folder": "downport",
  "input_filter": [
    "intf",
    "dtel",
    "clas"
  ],
  "output_folder": "output",
  "libs": [
    {
      "url": "https://github.com/open-abap/open-abap-core"
    }
  ],
  "write_unit_tests": true,
  "write_source_map": true,
  "options": {
    "ignoreSyntaxCheck": false,
    "addFilenames": true,
    "addCommonJS": true,
    "keywords": ["in", "default"],
    "unknownTypes": "runtimeError",
    "extraSetup": "../setup.mjs",
    "skip": []
  }
}
